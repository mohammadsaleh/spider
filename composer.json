{
    "name": "mohammadsaleh/spider",
    "description": "An useful package of plugins for CakePHP",
    "type": "cakephp-plugin",
	"license": "MIT",
	"homepage": "http://github.com/mohammadsaleh/spider",
    "require": {
        "php": ">=5.4.16",
        "cakephp/cakephp": "~3.0",
		"cakephp/acl": "dev-master",
		"friendsofcake/bootstrap-ui": "~0.3",
		"friendsofcake/search": "dev-master"
    },
	"authors": [
		{
			"name": "Mohammad Saleh",
			"email": "mohammadsaleh.sayari@gmail.com"
		}
	],
	"autoload": {
		"psr-4": {
			"Spider\\": "spider/Spider/src",
			"AclManager\\": "spider/AclManager/src",
			"PluginManager\\": "spider/PluginManager/src",
			"Settings\\": "spider/Settings/src",
			"Users\\": "spider/Users/src"
		}
	}
}
